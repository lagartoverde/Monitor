<mensaje>
  <emisor>
    <direccion>
      <ip>{{emisor.ip}}</ip>
      <puerto>{{emisor.puerto}}</puerto>
    </direccion>
    <rol>{{emisor.rol}}</rol>
  </emisor>
  <receptor>
    <direccion>
      <ip>{{receptor.ip}}</ip>
      <puerto>{{receptor.puerto}}</puerto>
      <id>{{receptor.id}}</id>
    </direccion>
    <rol>{{receptor.rol}}</rol>
  </receptor>
  <tipo>{{tipo}}</tipo>
  <hora>{{hora}}</hora>
  <cuerpo>
    {{cuerpo}}
  </cuerpo>
</mensaje>