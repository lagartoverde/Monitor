<listaCompra>
  {{#each productos}}
    <producto>
      <nombre>
        {{nombre}}
      </nombre>
      <cantidad>
        {{cantidad}}
      </cantidad>
    </producto>
    {{/each}}
</listaCompra>
<listaTiendas>
  {{#each tiendas}}
    <tienda>
      <direccion>
        <ip>{{ip}}</ip>
        <puerto>{{puerto}}</puerto>
        <id>{{id}}</id>
      </direccion>
    </tienda>
    {{/each}}
</listaTiendas>