<listaCompra>
  {{#each producto}}
    <producto>
      <nombre>
        {{nombre}}
      </nombre>
      <precio>
        {{precio}}
      </precio>
    </producto>
    {{/each}}
</listaCompra>
<listaTiendas>
  {{#each tienda}}
    <tienda>
        <direccion>
          <ip>{{ip}}</ip>
          <puerto>{{puerto}}</puerto>
        </direccion>
    </tienda>
    {{/each}}
</listaTiendas>